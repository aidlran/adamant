{
  "$schema": "https://raw.githubusercontent.com/conventional-changelog/conventional-changelog-config-spec/master/versions/2.2.0/schema.json",
  "types": [
    {
      "type": "docs",
      "section": "Documentation"
    },
    {
      "type": "feat",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Fixes"
    },
    {
      "type": "perf",
      "section": "Performance"
    },
    {
      "type": "chore",
      "hidden": true
    },
    {
      "type": "refactor",
      "hidden": true
    },
    {
      "type": "test",
      "hidden": true
    }
  ]
}
